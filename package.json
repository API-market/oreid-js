{
  "name": "oreid-js",
  "version": "4.1.1",
  "description": "Add authentication and signing to any blockchain app",
  "author": "AIKON",
  "license": "MIT",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "browser": {
    "crypto": false
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist/ && rm -rf dist.browser/ && rollup -c",
    "dev": "rollup -c -w",
    "build-browser": "rm -rf dist.browser/* && ./node_modules/.bin/ncc build -o dist.browser --minify",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint --fix \"src/**\"",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn run lint && yarn run test",
    "test": "jest --runInBand --coverage --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/api-market/oreid-js.git"
  },
  "bugs": {
    "url": "https://github.com/api-market/oreid-js/issues"
  },
  "homepage": "https://github.com/api-market/oreid-js#readme",
  "dependencies": {
    "@aikon/eos-transit": "^4.0.8",
    "@msgpack/msgpack": "^2.7.2",
    "@types/express": "^4.17.8",
    "axios": "^0.26.0",
    "canonicalize": "^1.0.8",
    "crypto-js": "^4.1.1",
    "dotenv": "^8.6.0",
    "js-base64": "^3.7.2",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "require-from-url": "^3.1.3",
    "semver": "^6.3.0",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@aikon/aikon-configs": "1.0.1",
    "@types/crypto-js": "^4.1.0",
    "@types/jest": "^26.0.14",
    "@types/js-base64": "^3.3.1",
    "@types/js-cookie": "^2.2.6",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^13.13.25",
    "@types/uuid": "^8.3.0",
    "@vercel/ncc": "^0.27.0",
    "express": "^4.17.1",
    "jest": "^26.0.0",
    "rollup": "^2.75.5",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-jest": "^26.4.1",
    "ts-node": "^8.5.4",
    "typescript": "^4.6.4"
  }
}