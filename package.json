{
  "name": "oreid-js",
  "version": "2.12.2",
  "description": "Add user authentication and signing to any blockchain app",
  "author": "AIKON",
  "license": "MIT",
  "main": "./dist/index.js",
  "browser": "./dist/browser.js",
  "files": [
    "dist",
    "dist.browser",
    "dist.webwidget"
  ],
  "scripts": {
    "build": "npm run build-server && npm run build-browser && npm run build-widget",
    "build-server": "rm -rf dist/* && tsc --build tsconfig.json",
    "build-browser": "rm -rf dist.browser/* && ./node_modules/.bin/ncc build -o dist.browser --minify",
    "build-widget": "rm -rf dist.widget/* && ./node_modules/.bin/ncc build ./dist/webwidget -o ./dist.webwidget --minify",
    "lint": "./node_modules/.bin/eslint src",
    "lint-fix": "./node_modules/.bin/eslint src --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test",
    "test": "jest --runInBand --coverage --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/api-market/oreid-js.git"
  },
  "bugs": {
    "url": "https://github.com/api-market/oreid-js/issues"
  },
  "homepage": "https://github.com/api-market/oreid-js#readme",
  "dependencies": {
    "@msgpack/msgpack": "^2.7.0",
    "@aikon/eos-transit": "^4.0.7",
    "@types/create-hmac": "^1.1.0",
    "@types/express": "^4.17.8",
    "@types/js-cookie": "^2.2.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/jwt-decode": "^2.2.1",
    "axios": "^0.21.1",
    "create-hmac": "^1.1.7",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.6",
    "js-base64": "^2.5.2",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "semver": "^6.3.0",
    "uuid": "^7.0.3",
    "aikon-oreid-shared": "git+https://apim-npmbot:LtT3pgcj9YAxMrNN@github.com/API-market/aikon-oreid-shared.git#1.2.7",
    "zoid": "^9.0.66"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^13.13.25",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "@vercel/ncc": "^0.27.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.3.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^26.0.0",
    "prettier": "^1.19.1",
    "ts-jest": "^26.4.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  }
}
