{
  "name": "oreid-js",
  "version": "3.2.0",
  "description": "Add authentication and signing for any blockchain app",
  "author": "AIKON",
  "license": "MIT",
  "main": "./dist/index.js",
  "browser": "./dist/browser.js",
  "files": [
    "dist",
    "dist.browser"
  ],
  "scripts": {
    "build": "npm run build-server && npm run build-browser",
    "build-server": "rm -rf dist/* && tsc --build tsconfig.json",
    "build-browser": "rm -rf dist.browser/* && ./node_modules/.bin/ncc build -o dist.browser --minify",
    "lint": "./node_modules/.bin/eslint src",
    "lint-fix": "./node_modules/.bin/eslint src --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test",
    "test": "jest --runInBand --coverage --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/api-market/oreid-js.git"
  },
  "bugs": {
    "url": "https://github.com/api-market/oreid-js/issues"
  },
  "homepage": "https://github.com/api-market/oreid-js#readme",
  "dependencies": {
    "@aikon/eos-transit": "^4.0.8",
    "@msgpack/msgpack": "^2.7.2",
    "axios": "^0.26.0",
    "canonicalize": "^1.0.8",
    "crypto-js": "^4.1.1",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.3.1",
    "js-base64": "^3.7.2",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "require-from-url": "^3.1.3",
    "semver": "^6.3.0",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.0",
    "@types/express": "^4.17.13",
    "@types/js-base64": "^3.3.1",
    "@types/js-cookie": "^2.2.6",
    "@types/jwt-decode": "^2.2.1",
    "@types/jest": "^26.0.14",
    "@types/node": "^13.13.25",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "@vercel/ncc": "^0.27.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.3.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^26.0.0",
    "prettier": "^1.19.1",
    "ts-jest": "^26.4.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  }
}